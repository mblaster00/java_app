<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>NoCacheFilter</filter-name>
  	<display-name>NoCacheFilter</display-name>
  	<description></description>
  	<filter-class>com.JavaWebApplication.Controller.NoCacheFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>NoCacheFilter</filter-name>
  	<url-pattern>/NoCacheFilter</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>RegisterServlet</servlet-name>
  	<display-name>Register</display-name>
  	<description></description>
  	<servlet-class>com.JavaWebApplication.Controller.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.JavaWebApplication.Controller.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Refresh</servlet-name>
  	<display-name>Refresh</display-name>
  	<description></description>
  	<servlet-class>com.JavaWebApplication.Controller.Refresh</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutServlet</servlet-name>
  	<display-name>LogoutServlet</display-name>
  	<description></description>
  	<servlet-class>com.JavaWebApplication.Controller.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegisterServlet</servlet-name>
  	<url-pattern>/Register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Refresh</servlet-name>
  	<url-pattern>/Refresh</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutServlet</servlet-name>
  	<url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
</web-app>
